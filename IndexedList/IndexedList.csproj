<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>Nico Thomas</Authors>
        <Description>This library provides a list implementation that allows you to create indices for properties of the list's element's type. It will automatically create dictionaries for the properties and allow for faster filtering.</Description>
        <PackageProjectUrl>https://github.com/nico1207/IndexedList</PackageProjectUrl>
        <RepositoryUrl>https://github.com/nico1207/IndexedList</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>list, collection, index, lookup, filter</PackageTags>
        <PackageLicenseUrl>https://github.com/nico1207/IndexedList/blob/main/LICENSE</PackageLicenseUrl>
    </PropertyGroup>

</Project>
